{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe 生成活动文件",
            "command": "D:\\mingw64\\bin\\g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-I${workspaceFolder}/include",
                "${file}",           // 这里就是您的runer/vector_test.cpp文件
                "-o",
                "${workspaceFolder}/exe/${fileBasenameNoExtension}.exe"
